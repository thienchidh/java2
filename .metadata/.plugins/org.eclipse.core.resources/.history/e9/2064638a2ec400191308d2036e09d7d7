package ex3.model;

public class Rectangle {
	private float width;
	private float height;

	public Rectangle() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Rectangle(float width, float height) {
		super();
		this.width = width;
		this.height = height;
	}

	public void input() {

	}

	public void display() {

	}

	public float caculateArea() {
		return height * width;
	}

	public float caculatePerimeter() {

	}

}
