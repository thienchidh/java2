package test;

import controls.EmploeeManagement;

public class TestEmployeeManager {

	public static void main(String[] args) {

		String s1 = "abc";
		String s2 = "cba";

		int i = 0;
		int j = s2.length() - 1;

		while (i < s1 && j >= 0) {

			i++;
			j--;
		}

		new EmploeeManagement().run();
	}
}
