package controls;

import java.util.Scanner;

public class MyManage {
	private Scanner sc = new Scanner(System.in);

	public void run() {
		while (true) {
			try {
				showMenu();
				int choice = Integer.parseInt(sc.nextLine().trim());

			} catch (Exception e) {
				System.err.println(e);
			}
		}
	}

}
