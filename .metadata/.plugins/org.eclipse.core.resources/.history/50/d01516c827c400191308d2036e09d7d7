package model;

import controls.IOutput;

public class Atoms implements IOutput {
	private int number;
	private String symbol;
	private int fullname;
	private int weight;

	public Atoms() {
		super();
	}

	public boolean accept() {
		// TODO
		return false;
	}

	@Override
	public String toString() {
		return String.format("%-10s%-10s%-10s\\n", number, symbol, fullname, weight);
	}

}
